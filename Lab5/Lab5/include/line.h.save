#ifndef LINE_H
#define LINE_H
#include <bits/stdc++.h>
#include "point.h"
using namespace std;

class line
{
    public:  //composition relation
        point p1;
        point p2 ;

        line()
        {
         p1.x=3;
         p1.y=4;

         p2.x=3;
         p2.y=7;


        }


       int getLength()
       {
         return sqrt(pow(p2.x - p1.x ,2) + (p2.y - p1.y ,2)  );

       }

};

#endif // LINE_H
